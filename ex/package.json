{
  "name": "portal-ng",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build:tst": "ng build --configuration tst",
    "build:acc": "ng build --configuration acc",
    "build:prd": "ng build --configuration prd",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:single": "ng test --watch=false",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d . --disableDependencies --minimal -t",
    "build:tokens": "style-dictionary build --config tokens.config.js && ng lint --fix",
    "lint": "ng lint",
    "lint:stylelint": "stylelint src/**/*.scss --fix",
    "fix": "prettier --write src/**/*.ts src/**/*.html !**/*-gen.ts !src/**/generated/*",
    "gql:codegen": "npm run gql:gateway-codegen && npm run gql:cms-codegen",
    "gql:gateway-codegen": "graphql-codegen --config codegen.yml",
    "gql:cms-codegen": "graphql-codegen --require dotenv/config --config cms-codegen.yml dotenv_config_path=src/environments/cms-dev.env",
    "translations-export": "ngx-translate-extract-csv -o ./translation.csv -l en-US,nl-NL,de-DE,es-ES,fr-FR,pl-PL,ru-RU,tr-TR,zh-Hans-CN",
    "translations-import": "ngx-translate-extract-csv -r -i ./translation.csv -o ./translatedJson",
    "update-csp:dev": "node scripts/update-csp.mjs src/index.dev.html scripts/dev.csp.json && npx prettier --write src/index.dev.html",
    "update-csp:prod": "node scripts/update-csp.mjs src/index.html scripts/prod.csp.json && npx prettier --write src/index.html",
    "update-csp:all": "npm run update-csp:dev && npm run update-csp:prod",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "husky": "cd ../.. && husky install src/portal-ng/.husky ",
    "pre-commit": "lint-staged",
    "check-branch-name": "bash ./hooks/check_branch_naming.sh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --color --fix -c .eslintrc.json"
    ],
    "*.scss": "stylelint --fix"
  },
  "dependencies": {
    "@angular-architects/module-federation": "^17.0.8",
    "@angular/animations": "17.2.1",
    "@angular/cdk": "^17.2.0",
    "@angular/common": "17.2.1",
    "@angular/compiler": "17.2.1",
    "@angular/core": "17.2.1",
    "@angular/forms": "17.2.1",
    "@angular/platform-browser": "17.2.1",
    "@angular/platform-browser-dynamic": "17.2.1",
    "@angular/router": "17.2.1",
    "@apollo/client": "^3.9.5",
    "@azure/abort-controller": "1.1.0",
    "@azure/storage-blob": "^12.14.0",
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@fontsource/noto-sans-sc": "^4.5.12",
    "@fontsource/roboto": "^4.5.8",
    "@ngneat/transloco": "^4.3.0",
    "@ngneat/transloco-locale": "^4.1.0",
    "@ngneat/transloco-messageformat": "^5.0.0",
    "@ngneat/transloco-persist-lang": "^4.0.0",
    "@ngneat/until-destroy": "^9.2.1",
    "@ngrx/effects": "^17.1.0",
    "@ngrx/router-store": "^17.1.0",
    "@ngrx/store": "^17.1.0",
    "@ngrx/store-devtools": "^17.1.0",
    "@vanderlande-gravity/components": "^0.12.20",
    "@vanderlande-gravity/core": "^0.12.20",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "angular-auth-oidc-client": "^16.0.0",
    "angular-svg-icon": "^17.0.0",
    "angular-svg-icon-preloader": "^7.0.0",
    "apollo-angular": "^6.0.0",
    "contentful": "^10.3.4",
    "graphql": "^16.8.1",
    "husky": "^8.0.3",
    "lit": "^2.7.6",
    "ngx-translate-extract-csv": "1.1.1",
    "rxjs": "^7.8.1",
    "svg-pan-zoom": "^3.6.1",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "vcard4": "^4.0.0",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "17.2.0",
    "@angular/compiler-cli": "17.2.1",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/named-operations-object": "^2.3.1",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
    "@graphql-codegen/typescript-msw": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@playwright/test": "^1.42.1",
    "@schematics/angular": "^17.0.6",
    "@types/jasmine": "^4.3.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.16.19",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "cheerio": "^1.0.0-rc.12",
    "csp-header": "^5.1.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^14.0.3",
    "junit-report-merger": "^6.0.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^2.2.14",
    "ngx-build-plus": "^17.0.0",
    "prettier": "^3.1.1",
    "prettier-eslint": "^16.1.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "pretty-quick": "^4.0.0",
    "puppeteer": "^19.7.1",
    "style-dictionary": "^3.7.2",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "typescript": "^5.2.2",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "msw": {
    "workerDirectory": [
      "src"
    ]
  }
}
